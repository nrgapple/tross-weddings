{
  "name": "tross-weddings",
  "version": "9.27.2",
  "private": false,
  "scripts": {
    "build:1-migrate": "prisma migrate deploy",
    "build:2-build": "next build",
    "build": "run-s build:*",
    "db-up": "docker-compose up -d",
    "db-seed": "prisma db seed",
    "db-migrate-dev": "yarn prisma migrate dev",
    "db-nuke": "docker-compose down --volumes --remove-orphans",
    "dev": "next dev",
    "dx": "run-s db-up db-migrate-dev db-seed dev",
    "start": "next start",
    "ts-node": "ts-node --compiler-options \"{\\\"module\\\":\\\"commonjs\\\"}\"",
    "postinstall": "prisma generate",
    "prettier": "prettier --write ./src"
  },
  "prisma": {
    "seed": "yarn ts-node prisma/seed.ts"
  },
  "prettier": {
    "printWidth": 80,
    "trailingComma": "all",
    "singleQuote": true,
    "semi": false,
    "arrowParens": "avoid"
  },
  "dependencies": {
    "@chakra-ui/react": "^2.3.2",
    "@emotion/react": "^11.10.4",
    "@emotion/styled": "^11.10.4",
    "@glideapps/glide-data-grid": "^5.0.0",
    "@next-auth/prisma-adapter": "^1.0.4",
    "@prisma/client": "^3.0.1",
    "@trpc/client": "^9.27.2",
    "@trpc/next": "^9.27.2",
    "@trpc/react": "^9.27.2",
    "@trpc/server": "^9.27.2",
    "ahooks": "^3.7.1",
    "clsx": "^1.1.1",
    "framer-motion": "^7.3.2",
    "lodash": "^4.17.21",
    "marked": "^4.1.0",
    "next": "^12.1.6",
    "next-auth": "^4.10.3",
    "radash": "^7.1.0",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "react-query": "^3.39.2",
    "react-responsive-carousel": "^3.2.23",
    "superjson": "^1.7.4",
    "zod": "^3.0.0"
  },
  "devDependencies": {
    "@types/faker": "^5.5.9",
    "@types/node": "^18.7.11",
    "@types/react": "^18.0.9",
    "faker": "^5.5.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.5.1",
    "prisma": "^4.2.1",
    "ts-node": "^10.3.0",
    "typescript": "4.4.4"
  },
  "publishConfig": {
    "access": "restricted"
  }
}
